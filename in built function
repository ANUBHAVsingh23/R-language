> a<-c(3,4,5,6)
> b<-c(7,8,9,10)
> d=a+b
> print(d)
> h<-c()
> 
> append(h,2)
> f=a*b
> print(f)
> print(sum(d))
> print(mean(d))
> print(prod(d))
> print(min(d))
> print(max(d))
> print(sort(d))
> print(ascending(d))
> print(sort(d))
> print(rev(d))
> print(c(a,b))
